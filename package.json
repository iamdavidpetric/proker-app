{
  "name": "proker-app",
  "version": "0.5.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "2.0.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "axios": "1.3.4",
    "env-cmd": "10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.7.1",
    "react-router-dom": "6.8.1",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.development react-scripts start",
    "build-s": "env-cmd -f ./.env.staging react-scripts build",
    "build-p": "env-cmd -f ./.env.production react-scripts build",
    "build": "react-scripts build",
    "clean": "rm -rf node_modules && yarn install",
    "eject": "react-scripts eject",
    "prettier:fix": "npx prettier --write ./"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "tailwindcss": "3.2.6"
  }
}
