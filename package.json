{
  "name": "proker-app",
  "version": "0.6.0",
  "private": true,
  "dependencies": {
    "@heroicons/react": "2.0.16",
    "@lagunovsky/redux-react-router": "4.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "axios": "1.3.2",
    "env-cmd": "10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "redux": "4.2.1",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-saga": "1.2.2",
    "reduxsauce": "1.2.1",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.development react-scripts start",
    "build-s": "env-cmd -f ./.env.staging react-scripts build",
    "build-p": "env-cmd -f ./.env.production react-scripts build",
    "build": "react-scripts build",
    "clean": "rm -rf node_modules && yarn install",
    "eject": "react-scripts eject",
    "prettier:fix": "npx prettier --write ./"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "tailwindcss": "3.2.6"
  }
}
